    "@types/leaflet": "^1.5.8",
    "@types/leaflet-polylinedecorator": "^1.6.0",
    "@types/mxgraph": "github:lgleim/mxgraph-typings",
    "@types/node": "~8.9.4",
    "@types/node-xlsx": "^0.15.0",
    "akita-schematics": "^2.0.1",
    "codelyzer": "~5.0.1",
    "concurrently": "^5.1.0",
    "cpx": "^1.5.0",
    "cypress": "^4.3.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "jest": "25.2.4",
    "jest-preset-angular": "^8.1.3",
    "prettier": "1.19.1",
    "ts-jest": "25.2.1",
    "ts-node": "~7.0.0",
    "tslint": "^6.1.1",
    "typescript": "3.8.3",
    "webpack-bundle-analyzer": "^3.6.1"
